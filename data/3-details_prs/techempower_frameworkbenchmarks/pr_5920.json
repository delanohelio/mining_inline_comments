+        $this->response->header('Content-Type', 'text/plain');\n+        return 'Hello, World!';\n+    }\n+\n+    public function db()\n+    {\n+        return $this->json(World::find(mt_rand(1, 10000)));\n+    }\n+\n+    public function fortunes()\n+    {\n+        $arr   = Fortune::findAll()->toArray();\n+        $rs    = [];\n+        $rs[0] = 'Additional fortune added at request time.';\n+        foreach ($arr as $item) {\n+            $rs[$item['id']] = $item['message'];\n+        }\n+        asort($rs);", "state": "SUBMITTED", "replyTo": null, "originalCommit": {"oid": "f2e7f3a9f548716728068da4aec2acb664514471"}, "originalPosition": 42}]}}, {"__typename": "PullRequestCommit", "commit": {"oid": "0e78a78effdff29c4b1821926adbc24de4c35040", "author": {"user": {"login": "lizhichao", "name": "vic"}}, "url": "https://github.com/TechEmpower/FrameworkBenchmarks/commit/0e78a78effdff29c4b1821926adbc24de4c35040", "committedDate": "2020-08-05T10:39:26Z", "message": "del toarr"}}, {"__typename": "PullRequestCommit", "commit": {"oid": "e31f42ed6b8575e646410051002848367f758471", "author": {"user": {"login": "lizhichao", "name": "vic"}}, "url": "https://github.com/TechEmpower/FrameworkBenchmarks/commit/e31f42ed6b8575e646410051002848367f758471", "committedDate": "2020-08-06T01:29:19Z", "message": "Get properties from object"}}, {"__typename": "PullRequestCommit", "commit": {"oid": "02cd2441f0861e53b5fbab9adb375a53b259c528", "author": {"user": {"login": "lizhichao", "name": "vic"}}, "url": "https://github.com/TechEmpower/FrameworkBenchmarks/commit/02cd2441f0861e53b5fbab9adb375a53b259c528", "committedDate": "2020-08-08T14:20:39Z", "message": "Update IndexController.php"}}, {"__typename": "PullRequestReview", "id": "MDE3OlB1bGxSZXF1ZXN0UmV2aWV3NDYzODIyNDY4", "url": "https://github.com/TechEmpower/FrameworkBenchmarks/pull/5920#pullrequestreview-463822468", "createdAt": "2020-08-08T19:24:53Z", "commit": {"oid": "02cd2441f0861e53b5fbab9adb375a53b259c528"}, "state": "APPROVED", "comm