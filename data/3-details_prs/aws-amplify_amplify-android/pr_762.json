,10 @@ private Completable drainMutationOutbox() {\n             HubChannel.DATASTORE,\n             HubEvent.create(DataStoreChannelEventName.PUBLISHED_TO_CLOUD, processedMutation)\n         );\n+        Amplify.Hub.publish(\n+            HubChannel.DATASTORE,\n+            HubEvent.create(DataStoreChannelEventName.OUTBOX_MUTATION_PROCESSED, processedMutation)", "state": "SUBMITTED", "replyTo": null, "originalCommit": {"oid": "3aec8acef206c45811c50252d298b96da83c385f"}, "originalPosition": 6}]}}, {"__typename": "PullRequestReview", "id": "MDE3OlB1bGxSZXF1ZXN0UmV2aWV3NDc2ODUxMjA4", "url": "https://github.com/aws-amplify/amplify-android/pull/762#pullrequestreview-476851208", "createdAt": "2020-08-27T15:42:53Z", "commit": {"oid": "3aec8acef206c45811c50252d298b96da83c385f"}, "state": "COMMENTED", "comments": {"totalCount": 0, "pageInfo": {"startCursor": null, "endCursor": null, "hasNextPage": false, "hasPreviousPage": false}, "nodes": []}}, {"__typename": "PullRequestCommit", "commit": {"oid": "3bfe3e06787015099776f18e60003ee16d81a869", "author": {"user": {"login": "raphkim", "name": "Raphael Kim"}}, "url": "https://github.com/aws-amplify/amplify-android/commit/3bfe3e06787015099776f18e60003ee16d81a869", "committedDate": "2020-08-27T19:07:26Z", "message": "update hub event payload format"}}, {"__typename": "PullRequestCommit", "commit": {"oid": "5905f41a2f887472369dab7b9e0d127ae559b0f9", "author": {"user": {"login": "raphkim", "name": "Raphael Kim"}}, "url": "https://github.com/aws-amplify/amplify-android/commit/5905f41a2f887472369dab7b9e0d127ae559b0f9", "committedDate": "2020-08-27T19:20:42Z", "message": "fix test filter to recognize new event format"}}, {"__typename": "PullRequestReview", "id": "MDE3OlB1bGxSZXF1ZXN0UmV2aWV3NDc3MDcwNDky", "url": "https://github.com/aws-amplify/amplify-android/pull/762#pullrequestreview-477070492", "createdAt": "2020-08-27T20:49:17Z", "commit": {"oid": "5905f41a2f887472369dab7b9e0d127ae559b0f9"}, "state": "APPROVED", "comments": {"total