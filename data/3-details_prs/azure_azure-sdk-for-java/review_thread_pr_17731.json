ncies>\n   <build>\n     <plugins>\n+      <plugin>", "state": "SUBMITTED", "replyTo": {"id": "MDI0OlB1bGxSZXF1ZXN0UmV2aWV3Q29tbWVudDUyNzk5OTA5Mw=="}, "originalCommit": {"oid": "985d61b9204988d58dece6bd982bd151a75247f3"}, "originalPosition": 4}, {"id": "MDI0OlB1bGxSZXF1ZXN0UmV2aWV3Q29tbWVudDUyODAxMzQ2Nw==", "bodyText": "We are still debating the namespace for this library. So, I think we can skip this library from coverage checks until the namespace is decided.", "url": "https://github.com/Azure/azure-sdk-for-java/pull/17731#discussion_r528013467", "createdAt": "2020-11-20T22:55:23Z", "author": {"login": "srnagar"}, "path": "eng/jacoco-test-coverage/pom.xml", "diffHunk": "@@ -531,6 +531,22 @@\n   </dependencies>\n   <build>\n     <plugins>\n+      <plugin>", "state": "SUBMITTED", "replyTo": {"id": "MDI0OlB1bGxSZXF1ZXN0UmV2aWV3Q29tbWVudDUyNzk5OTA5Mw=="}, "originalCommit": {"oid": "985d61b9204988d58dece6bd982bd151a75247f3"}, "originalPosition": 4}]}}]}}}, "rateLimit": {"limit": 5000, "remaining": 2798, "cost": 1, "resetAt": "2021-11-12T11:57:46Z"}}}