aries: [],\n+              spanCount: 10,\n+              width: 10,\n+              root: {\n+                serviceName: 'routing',\n+                spanName: 'post /location/update/v4',\n+              },\n+            }}\n+          />\n+        </tbody>\n+      </table>,\n+    );\n+\n+    const startTimeFormat = queryByTestId('TraceSummaryRow-startTimeFormat');\n+    expect(startTimeFormat).toBeInTheDocument();\n+    // Don't assert on hour as the timezone will be different in CI\n+    expect(startTimeFormat).toHaveTextContent(/10\\/2[34] [0-9][0-9]:52:55:237/);", "state": "SUBMITTED", "replyTo": null, "originalCommit": {"oid": "b7f7f346426bf425f3f01874e078d3e9c1ab492b"}, "originalPosition": 45}]}}, {"__typename": "PullRequestCommit", "commit": {"oid": "ffea1bbaec0ba8cf899351dbb56d2588942ebe8c", "author": {"user": null}, "url": "https://github.com/openzipkin/zipkin/commit/ffea1bbaec0ba8cf899351dbb56d2588942ebe8c", "committedDate": "2020-10-14T23:55:34Z", "message": "whoops"}}]}}}, "rateLimit": {"limit": 5000, "remaining": 1322, "cost": 1, "resetAt": "2021-11-01T15:33:45Z"}}}