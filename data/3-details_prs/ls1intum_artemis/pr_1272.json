Commit": {"oid": "4ce6aa1b86f65df3f73f3a72579f13be68dfedbd"}, "originalPosition": 5}, {"id": "MDI0OlB1bGxSZXF1ZXN0UmV2aWV3Q29tbWVudDM5NTMyODg2NQ==", "bodyText": "Please add multi language support", "url": "https://github.com/ls1intum/Artemis/pull/1272#discussion_r395328865", "createdAt": "2020-03-19T21:26:01Z", "author": {"login": "TobiasPr"}, "path": "src/main/webapp/app/admin/user-management/user-management.component.html", "diffHunk": "@@ -7,6 +7,10 @@ <h2>\n     </h2>\n     <jhi-alert></jhi-alert>\n     <div class=\"table-responsive\" *ngIf=\"users\">\n+        <div class=\"form-group form-inline\">\n+            Search for User: <input class=\"form-control ml-2\" type=\"text\" name=\"searchUser\" [(ngModel)]=\"searchTerm\" />\n+            <span class=\"ml-3\" *ngIf=\"loading\">Loading...</span>", "state": "SUBMITTED", "replyTo": null, "originalCommit": {"oid": "4ce6aa1b86f65df3f73f3a72579f13be68dfedbd"}, "originalPosition": 6}, {"id": "MDI0OlB1bGxSZXF1ZXN0UmV2aWV3Q29tbWVudDM5NTMyOTg5OQ==", "bodyText": "maybe loadingSearchResult for more descriptive name?", "url": "https://github.com/ls1intum/Artemis/pull/1272#discussion_r395329899", "createdAt": "2020-03-19T21:28:32Z", "author": {"login": "TobiasPr"}, "path": "src/main/webapp/app/admin/user-management/user-management.component.ts", "diffHunk": "@@ -10,12 +10,15 @@ import { AccountService } from 'app/core/auth/account.service';\n import { Subject } from 'rxjs';\n import { ITEMS_PER_PAGE } from 'app/shared/constants/pagination.constants';\n import { AlertService } from 'app/core/alert/alert.service';\n+import { SortingOrder } from 'app/shared/table/pageable-table';\n \n @Component({\n     selector: 'jhi-user-management',\n     templateUrl: './user-management.component.html',\n })\n export class UserManagementComponent implements OnInit, OnDestroy {\n+    loading = false;", "state": "SUBMITTED", "replyTo": null, "originalCommit": {"oid": "4ce6aa1b86f65df3f73f3a72579f13be68dfedbd"}, "originalPosition": 11}, {"id": "MDI0OlB1bGxSZXF1ZXN0UmV2aWV3Q29tbWVudDM5NTMzMTE2NQ==", "bodyText": "loading should be set to false in case of an error", "url": "https://github.com/ls1intum/Artemis/pull/1272#discussion_r395331165", "createdAt": "2020-03-19T21:31:12Z", "author": {"login": "TobiasPr"}, "path": "src/main/webapp/app/admin/user-management/user-management.component.ts", "diffHunk": "@@ -100,14 +105,20 @@ export class UserManagementComponent implements OnInit, OnDestroy {\n      * Retrieve the list of users from the user service for a single page in the user management based on the page, size and sort configuration\n      */\n     loadAll() {\n+        this.loading = true;\n         this.userService\n             .query({\n                 page: this.page - 1,\n-                size: this.itemsPerPage,\n-                sort: this.sort(),\n+                pageSize: this.itemsPerPage,\n+                searchTerm: this.searchTermString,\n+                sortingOrder: this.reverse ? SortingOrder.ASCENDING : SortingOrder.DESCENDING,\n+                sortedColumn: this.predicate,\n             })\n             .subscribe(\n-                (res: HttpResponse<User[]>) => this.onSuccess(res.body!, res.headers),\n+                (res: HttpResponse<User[]>) => {\n+                    this.loading = false;\n+                    this.onSuccess(res.body!, res.headers);\n+                },\n                 (res: HttpErrorResponse) => onError(this.alertService, res),", "state": "SUBMITTED", "replyTo": null, "originalCommit": {"oid": "4ce6aa1b86f65df3f73f3a72579f13be68dfedbd"}, "originalPosition": 53}, {"id": "MDI0OlB1bGxSZXF1ZXN0UmV2aWV3Q29tbWVudDM5NTMzMTY2Mw==", "bodyText": "searchTermString = '' saves initialization in constructor", "url": "https://github.com/ls1intum/Artemis/pull/1272#discussion_r395331663", "createdAt": "2020-03-19T21:32:10Z", "author": {"login": "TobiasPr"}, "path": "src/main/webapp/app/admin/user-management/user-management.component.ts", "diffHunk": "@@ -28,6 +31,7 @@ export class UserManagementComponent implements OnInit, OnDestroy {\n     predicate: string;\n     previousPage: number;\n     reverse: boolean;\n+    searchTermString: string;", "state": "SUBMITTED", "replyTo": null, "originalCommit": {"oid": "4ce6aa1b86f65df3f73f3a72579f13be68dfedbd"}, "originalPosition": 20}, {"id": "MDI0OlB1bGxSZXF1ZXN0UmV2aWV3Q29tbWVudDM5NTMzNzY3MQ==", "bodyText": "For every key event, a server request is started. Maybe we could reduce that with rxjs debounce time if we do another implementation of how the search is triggered?", "url": "https://github.com/ls1intum/Artemis/pull/1272#discussion_r395337671", "createdAt": "2020-03-19T21:45:51Z", "author": {"login": "TobiasPr"}, "path": "src/main/webapp/app/admin/user-management/user-management.component.ts", "diffHunk": "@@ -100,14 +105,20 @@ export class UserManagementComponent implements OnInit, OnDestroy {\n      * Retrieve the list of users from the user service for a single page in the user management based on the page, size and sort configuration\n      */\n     loadAll() {\n+        this.loading = true;\n         this.userService", "state": "SUBMITTED", "replyTo": null, "originalCommit": {"oid": "4ce6aa1b86f65df3f73f3a72579f13be68dfedbd"}, "originalPosition": 37}, {"id": "MDI0OlB1bGxSZXF1ZXN0UmV2aWV3Q29tbWVudDM5NTM0MTA2NQ==", "bodyText": "this method is used by a lot of other methods. Are you sure it's save to do the changes you did?", "url": "https://github.com/ls1intum/Artemis/pull/1272#discussion_r395341065", "createdAt": "2020-03-19T21:54:02Z", "author": {"login": "TobiasPr"}, "path": "src/main/webapp/app/shared/util/request-util.ts", "diffHunk": "@@ -4,15 +4,8 @@ export const createRequestOption = (req?: any): HttpParams => {\n     let options: HttpParams = new HttpParams();\n     if (req) {\n         Object.keys(req).forEach(key => {\n-            if (key !== 'sort') {\n-                options = options.set(key, req[key]);\n-            }\n+            options = options.set(key, req[key